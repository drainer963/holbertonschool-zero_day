<script>function isCookieSet(e){return-1!==document.cookie.indexOf(e+"=")}function isValidEmailAddress(e){return new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i).test(e)}function serialize(e){if(e&&"FORM"===e.nodeName){var t,n,o=[];for(t=e.elements.length-1;0<=t;t-=1)if(""!==e.elements[t].name)switch(e.elements[t].nodeName){case"INPUT":switch(e.elements[t].type){case"text":case"hidden":case"password":case"button":case"reset":case"submit":o.push(e.elements[t].name+"="+encodeURIComponent(e.elements[t].value));break;case"checkbox":case"radio":e.elements[t].checked&&o.push(e.elements[t].name+"="+encodeURIComponent(e.elements[t].value))}break;case"TEXTAREA":o.push(e.elements[t].name+"="+encodeURIComponent(e.elements[t].value));break;case"SELECT":switch(e.elements[t].type){case"select-one":o.push(e.elements[t].name+"="+encodeURIComponent(e.elements[t].value));break;case"select-multiple":for(n=e.elements[t].options.length-1;0<=n;n-=1)e.elements[t].options[n].selected&&o.push(e.elements[t].name+"="+encodeURIComponent(e.elements[t].options[n].value))}break;case"BUTTON":switch(e.elements[t].type){case"reset":case"submit":case"button":o.push(e.elements[t].name+"="+encodeURIComponent(e.elements[t].value))}}return o.join("&")}}function detectIE(){var e=window.navigator.userAgent,t=e.indexOf("MSIE ");if(0<t&&parseInt(e.substring(t+5,e.indexOf(".",t)),10)<10)return!0;var n=null!=navigator.userAgent.match(/iPad/i),o=null!=navigator.userAgent.match(/Android/i),u=null!=navigator.userAgent.match(/iPhone/i),i=null!=navigator.userAgent.match(/bot/i);return!!(n||o||u||i)}function hideAndSetCookie(){var e=document.querySelectorAll(".email-overlay"),t=document.querySelectorAll(".overlay-wrapper");e[0].style.display="none",t[0].style.display="none";var n=new Date;n.setTime(n.getTime()+2592e6);var o="htg_newsletter_subform_hide=1; expires="+n.toGMTString()+";path=/";document.cookie=o}function keyDownModal(e){27==e.keyCode&&hideAndSetCookie()}var isInViewport=function(e){var t=e.getBoundingClientRect();return 0<=t.top&&0<=t.left&&t.bottom<=(window.innerHeight||document.documentElement.clientHeight)&&t.right<=(window.innerWidth||document.documentElement.clientWidth)};function showEmail(){var e=Math.max(document.documentElement.clientWidth,window.innerWidth||0),t=document.querySelectorAll("#emailoverlayformbutton"),n=document.querySelectorAll("#emailoverlayform"),o=document.querySelectorAll(".email-overlay"),u=document.querySelectorAll(".overlay-wrapper");!isCookieSet("htg_newsletter_subform_hide")&&!isCookieSet("htgreader")&&!detectIE()&&768<e&&(o[0].style.display="block",u[0].style.display="block"),t[0].addEventListener("click",function(e){var t=document.querySelectorAll("#overlayform"),n=(document.querySelectorAll("#emailoverlayform"),document.querySelectorAll(".overlay-message")),o=t[0].value;if(!isValidEmailAddress(o)||null!=o.match(/fuck/i))return t[0].style.background="lightpink",n[0].innerHTML="This email address is invalid.",e.preventDefault(),!1}),n[0].addEventListener("submit",function(e){return xhr=new XMLHttpRequest,xhr.open("POST",window.location.origin+"/em/subscribe"),xhr.setRequestHeader("Content-Type","application/x-www-form-urlencoded"),xhr.withCredentials=!0,xhr.onload=function(){200===xhr.status?(n[0].style.display="none",document.querySelectorAll(".overlay-message")[0].innerHTML="Thanks for subscribing! You'll get your first issue shortly!",setTimeout(function(){hideAndSetCookie()},2e3)):200!==xhr.status&&hideAndSetCookie()},xhr.send(serialize(n[0])),e.preventDefault(),!1})}document.addEventListener("keydown",keyDownModal,!1),document.addEventListener("DOMContentLoaded",function(e){var t=document.querySelectorAll(".email-overlay, #emailclose"),n=Math.max(document.documentElement.clientWidth,window.innerWidth||0);for(i=0;i<t.length;i++)t[i].addEventListener("click",function(){hideAndSetCookie()});if(!isCookieSet("htg_newsletter_subform_hide")&&!isCookieSet("htgreader")&&!detectIE()){var o=document.querySelector(".social-media-buttons");window.addEventListener("scroll",function(e){isInViewport(o)&&setTimeout(function(){showEmail()},3e3)},!1)}setTimeout(function(){!isCookieSet("htg_newsletter_subform_hide")&&!isCookieSet("htgreader")&&!detectIE()&&768<n&&document.querySelectorAll("html")[0].addEventListener("mouseleave",function(){showEmail()})},6e4)});</script>
